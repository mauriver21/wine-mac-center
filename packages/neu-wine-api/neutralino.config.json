{
  "applicationId": "neu-wine-api",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 3001,
  "documentRoot": "/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "filesystem.*",
    "computer.*"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/public/icon.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "neu-wine-api",
    "resourcesPath": "/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.13.0",
    "clientVersion": "3.11.0",
    "frontendLibrary": {
      "patchFile": "/index.html",
      "devUrl": "http://localhost:3001"
    }
  }
}
