{
  "applicationId": "library-ui",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 3002,
  "documentRoot": "/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "singlePageServe": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "filesystem.*",
    "computer.*"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/public/icon.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "library-ui",
    "resourcesPath": "/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/public/neutralino.js",
    "binaryVersion": "5.3.0",
    "clientVersion": "5.3.0"
  }
}
